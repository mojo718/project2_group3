<button class="btn btn-primary" id="createLineup">Create New Lineup</button>
<br>
 <div class="row text-center">
<h1>Your Lineups</h1>
<br>
{{#if lineups.length }}
{{#each lineups as |lineup| }}
<div class="row">
  <table class="table table-hover table-primary table-striped">
  <thead>
    <tr>
      <th scope="col">Order</th>
      <th scope="col">Name</th>
      <th scope="col">Game Date</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>{{ lineup.player1 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">2</th>
      <td>{{ lineup.player2 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">3</th>
      <td>{{ lineup.player3 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">4</th>
      <td>{{ lineup.player4 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">5</th>
      <td>{{ lineup.player5 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">6</th>
      <td>{{ lineup.player6 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">7</th>
      <td>{{ lineup.player7 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">8</th>
      <td>{{ lineup.player8 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
       <tr>
      <th scope="row">9</th>
      <td>{{ lineup.player9 }}</td>
      <td> {{ lineup.game_date }} </td>
    </tr>
  </tbody>
</table>
</div>
<br>
{{/each}}
{{else}}
<div class="row text-center">
  <h1 style="color:red">No Lineups created. Plesae create a lineup first.</h1>
</div>
{{/if}}

<script src="./js/homepage.js"></script>